{
  "name": "foxford-downloader",
  "version": "1.0.0",
  "description": "Downloader utility for foxford courses",
  "bin": "out/main.js",
  "scripts": {
    "build": "export PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true && npm ci && babel src/ -d out && pkg . --out-path build/ && rm -rf out/ && rm -rf node_modules/"
