  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-rc.1",
    "chalk": "^2.4.1",
    "pkg": "^4.3.4",
    "prompt-sync": "^4.1.6",
    "puppeteer": "^1.7.0",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "slug": "^0.9.1",
    "sqlite3": "^4.0.2"
  }
}
